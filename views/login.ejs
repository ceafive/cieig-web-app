<% include partials/header %>


<div class="container">
<div class="row"></div>
<h1 class="text-center">
	<strong>Login</strong>
</h1>

<form class="form center-block" action="/login" method="POST">
	<div class="form-group">
		<label class="big" for="username">Username:</label>
		<input id="username" class="form-control input-sm" type="text" placeholder="username" name="username" required>
	</div>
	<div class="form-group">
		<label class="big" for="image">Password:</label>
		<input id="image" class="form-control input-sm" type="password" placeholder="password" name="password" required>
	</div>
	<div class="form-group">
		<button class="btn btn-primary btn-block">Login</button>
	</div>
</form>

</div>	
</div>

<% include partials/footer %>